{
  "name": "toko-komputer-management",
  "version": "1.0.0",
  "description": "Sistem Manajemen Toko Komputer",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "seed": "node server/seeders/runSeed.js",
    "build": "cd client && npm install && npm run build",
    "vercel-build": "cd client && npm install && npm run build && mv build ../client/build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "@prisma/client": "^6.7.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "prisma": "^6.7.0",
    "supabase": "^2.22.12"
  }
}
